# Look up last apod used

#!/usr/bin/python
import os

directory = os.path.expanduser('~/apod/')
f = open(directory+'current','r')
n = int(f.readline())
f.close()

f = open(directory+'history','r')
pictures = f.readlines()
f.close()

picture = pictures[-n].split('/')[-1].split('-')

name = picture[0][2:] + picture[1] + picture[2][:2]

os.system('xdg-open http://apod.nasa.gov/apod/ap%s.html' %name)
